{
  "name": "the-shop/fortytwo",
  "description": "Boilerplate of 42 framework",
  "license": "MIT",
  "type": "project",
  "homepage": "https://github.com/the-shop/fortytwo",
  "authors": [
    {
      "name": "The Shop d.o.o.",
      "homepage": "http://the-shop.io/"
    }
  ],
  "require": {
    "php": "^7.1",
    "sentry/sentry": "1.7.1",
    "guzzlehttp/guzzle": "^6.3",
    "symfony/dotenv": "3.3.6",
    "php-amqplib/php-amqplib": "^2.7",
    "zendframework/zend-stdlib": "^3.1.0",
    "nikic/fast-route": "1.2",
    "mustache/mustache": "^2.12",
    "symfony/http-foundation": "^3.3",
    "firebase/php-jwt": "^5.0",
    "mongodb/mongodb": "1.1.2",
    "aws/aws-sdk-php": "^3.36",
    "sendgrid/sendgrid": "~6.0"
  },
  "require-dev": {
    "squizlabs/php_codesniffer": "^3.0",
    "phpunit/phpunit": "^6.3"
  },
  "autoload": {
    "psr-4": {
      "Application\\": "src/",
      "Application\\Configuration\\": "config/",
      "Framework\\Base\\":"vendor/the-shop/fortytwo-base/src/",
      "Framework\\Terminal\\":"vendor/the-shop/fortytwo-terminal/src/",
      "Framework\\Http\\":"vendor/the-shop/fortytwo-http/src/",
      "Framework\\RestApi\\":"vendor/the-shop/fortytwo-restapi/src/",
      "Framework\\CrudApi\\":"vendor/the-shop/fortytwo-crudapi/src/",
      "Framework\\Mongo\\":"vendor/the-shop/fortytwo-mongo/src/",
      "Framework\\SendGrid\\":"vendor/the-shop/fortytwo-sendgrid/src/",
      "Framework\\S3FU\\":"vendor/the-shop/fortytwo-s3fu/src/"
    }
  }
}
